<Window title="Stretching Example">
	<Container size="300,300">
		<Icon name="icon" scale="1" image="gui/icons/pychan_logo.png"/>
	</Container>
	<Label text="Move the sliders around to stretch the icon."/>
	<HBox><Label hexpand="0" text="Width:"/><Label name="wvalue"/></HBox>
	<Slider name="wslider" scale_start="9.6" scale_end="300"/>
	<HBox><Label hexpand="0" text="Height:"/><Label name="hvalue"/></HBox>
	<Slider name="hslider" scale_start="9.6" scale_end="300"/>
	<Button name="closeButton" text="Close"/>
</Window>
