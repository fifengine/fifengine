<template name="window" content="content">
<head>
	<link type="text/rcss" href="base_window_style.rcss"/>
	<script>
def onLoadWindow(element):
	self.owner_document.GetElementById('title').inner_rml = self.title
	</script>
</head>
<body class="window" onload="onLoadWindow(self)">
	<div id="title_bar">
		<handle move_target="#document">
			<span id="title"></span>
		</handle>
	</div>
	<div id="window">
		<div id="content">
		</div>
	</div>
</body>
</template>