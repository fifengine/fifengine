<Panel title="Object editor" position="10,200" min_size="200,250"> <!-- size="200,250" min_size="200,250" max_size="200,250" > -->
	<HBox>
		<Spacer min_size="150"/>
		<ImageButton up_image="gui/icons/help.png" down_image="gui/icons/help.png" hover_image="gui/icons/help.png" name="show_help" text="      Help " />
<!--
		<ImageButton name="show_settings" text="Settings" />
-->
	</HBox>

	<VBox hexpand="2">
		<Label text=" Namespace" min_size="80,15"/>
		<Label text="None" name="object_namespace"/>
		<Label text=" Object id" min_size="80,15"/>
		<Label text="None" name="object_id"/>
	</VBox>

	<HBox hexpand="2">
		<VBox>
			<Label text=" Cost id" min_size="80,20"/>
			<Label text=" Cost" min_size="80,20"/>
			<Label text=" Cellstack position" min_size="80,15"/>
			<Label text=" Static" min_size="80,15"/>
			<Label text=" Blocking " min_size="35,20"/>
		</VBox>
		
		<VBox hexpand="2">
			<TextBox text="None" name="cost_id"/>
			<TextBox text="None" name="cost_value"/>
			<Label text="None" name="object_cellstack_pos"/>
			<Label text="0" name="object_static" />
			<CheckBox name="object_blocking_toggle" marked="0"/>
		</VBox>
	
	</HBox>

	<VBox padding="0" name="actions_wrapper" hexpand="1">
		<Label text="           Actions:" background_color="20,20,20" />
		<ScrollArea min_size="85,40" horizontal_scrollbar="0">
			<ListBox name="select_actions" />
		</ScrollArea>
	</VBox>		

	<VBox padding="0" name="rotations_wrapper" hexpand="1">
		<Label text="           Rotations:" background_color="20,20,20" />
		<ScrollArea min_size="85,40" horizontal_scrollbar="0">
			<ListBox name="select_rotations" />
		</ScrollArea>
	</VBox>		

	<Label text="           Offset for current rotation:" background_color="20,20,20" />
	
	<HBox>
		<Label text=" x: " min_size="25,20"/>
		<TextBox text="0" name="x_offset" size="30,20" min_size="30,20" max_size="30,20" />
		<Button name="x_offset_up" text="+" max_size="20,20"/>
		<Button name="x_offset_dn" text="-" max_size="20,20"/>
	</HBox>

	<HBox>
		<Label text=" y: " min_size="25,20"/>
		<TextBox text="0" name="y_offset" size="30,20" min_size="30,20" max_size="30,20"/>
		<Button name="y_offset_up" text="+" max_size="20,20"/>
		<Button name="y_offset_dn" text="-" max_size="20,20"/>
	</HBox>

	<Button name="change_data" text="Save object data"/>
